{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "suspense": {
      "projectType": "library",
      "root": "projects/suspense",
      "sourceRoot": "projects/suspense/src",
      "prefix": "nbl",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/suspense/tsconfig.lib.json",
            "project": "projects/suspense/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/suspense/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/suspense/src/test.ts",
            "tsConfig": "projects/suspense/tsconfig.spec.json",
            "karmaConfig": "projects/suspense/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/suspense/tsconfig.lib.json",
              "projects/suspense/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "misc": {
      "projectType": "library",
      "root": "projects/misc",
      "sourceRoot": "projects/misc/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/misc/tsconfig.lib.json",
            "project": "projects/misc/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/misc/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/misc/src/test.ts",
            "tsConfig": "projects/misc/tsconfig.spec.json",
            "karmaConfig": "projects/misc/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/misc/tsconfig.lib.json",
              "projects/misc/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "flex": {
      "projectType": "library",
      "root": "projects/flex",
      "sourceRoot": "projects/flex/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/flex/tsconfig.lib.json",
            "project": "projects/flex/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/flex/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/flex/src/test.ts",
            "tsConfig": "projects/flex/tsconfig.spec.json",
            "karmaConfig": "projects/flex/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/flex/tsconfig.lib.json",
              "projects/flex/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "suspense"
}
